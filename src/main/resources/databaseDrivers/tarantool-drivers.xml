<?xml version="1.0" encoding="UTF-8"?>
<drivers>
    <!-- icon="TIcons.Tarantool13"-->
    <driver id="tarantool"
            name="tarantool"
            icon="TarantoolIcons.Tarantool13"
            dialect="tarantool"
            driver-class="org.tarantool.jdbc.SQLDriver">
        <url-template name="URL" template="jdbc:tarantool://{host}:{port}"/>
        <artifact name="tarantool" use="true" rolling="true"/>
        <option name="auto-sync" value="true"/>
    </driver>
</drivers>